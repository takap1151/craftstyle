<h1>Session#new</h1>
<%= form_tag admin_sessions_path do %>
  <div class="field">
    <%= label_tag :email, 'email' %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field"> 
    <%= label_tag :pass, 'password' %>
    <%= password_field_tag :pass , params[:pass]%>
  </div>
  <div class="actions">
    <%= submit_tag "ログイン" %>
  </div>
<% end %>
